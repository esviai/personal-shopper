			<h2>Update Item</h1>
				<div class="row">
			    <form class="col s12" action="" method="post">
			      <div class="row">
			        <div class="input-field col s5">
			          <input placeholder="Item Name" name="name" type="text" class="validate">
			        </div>
			      </div>
			      <div class="row">
			        <div class="input-field col s5">
			          <input placeholder="Item Price" name="price" type="text" class="validate">
			        </div>
			      </div>
			      <div class="row">
							<div class="input-field col s5">
			          <input placeholder="Category" name="category" type="text" class="validate">
			        </div>
			      </div>
						<div class="row">
							<div class="input-field col s5">
			          <input placeholder="Stock" name="stock" type="text" class="validate">
			        </div>
			      </div>
						<div class="row">
							<div class="input-field col s5">
			          <input placeholder="Description" name="description" type="text" class="validate">
			        </div>
			      </div>
						<a href="" class="waves-effect waves-light btn" name="submit">button</a>
			    </form>
			  </div>

      <!--Import jQuery before materialize.js-->
      <script type="text/javascript" src="https://code.jquery.com/jquery-2.1.1.min.js"></script>
